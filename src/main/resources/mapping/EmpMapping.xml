<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper  namespace="com.eduask.dao.EmpDao">
    <cache  eviction="LRU" flushInterval="500" size="50" readOnly="true"></cache>
    <select id="selectEmp" resultType="com.eduask.beans.Emp">
        select * from Emp
    </select>
    <insert id="insertEmp" parameterType="com.eduask.beans.Emp">
        insert into emp(emp_name,sex,address,dept_id) VALUES(#{emp_name},#{sex},#{address},#{dept.dept_id})
    </insert>
</mapper>